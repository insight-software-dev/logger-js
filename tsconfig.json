{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "noEmit": true,
    "allowJs": true,
    "checkJs": false
  },
  "include": ["logger.d.ts"],
  "exclude": ["node_modules"]
}

